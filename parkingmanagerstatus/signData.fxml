<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane id="BoarderPane" styleClass="bodyBg" stylesheets="@application.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parkingmanagerstatus.signData">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="27.0" layoutY="503.0" mnemonicParsing="false" onAction="#goMainView" prefHeight="79.0" prefWidth="102.0" style="-fx-background-color: #7c7393;" text="Back" textFill="#4d1b1b">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <TableView fx:id="signsData" layoutX="16.0" layoutY="99.0" prefHeight="403.0" prefWidth="574.0">
              <columns>
                <TableColumn fx:id="signID" prefWidth="75.0" text="SignID" />
                <TableColumn fx:id="subSignID" text="SubSignID" />
                  <TableColumn fx:id="status" prefWidth="71.0" text="Status" />
                  <TableColumn fx:id="counter" minWidth="0.0" prefWidth="91.0" text="Counter" />
                  <TableColumn fx:id="configuration" prefWidth="126.0" text="Configuration" />
                  <TableColumn fx:id="date" prefWidth="129.0" text="Timestamp" />
              </columns>
            </TableView>
            <TreeView fx:id="ParkTree" layoutX="598.0" layoutY="50.0" prefHeight="448.0" prefWidth="197.0" />
            <Button layoutX="153.0" layoutY="503.0" mnemonicParsing="false" onAction="#refreshWindow" prefHeight="79.0" prefWidth="102.0" style="-fx-background-color: #7c7393;" text="Refresh" textFill="#4d1b1b">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="291.0" layoutY="14.0" prefHeight="60.0" prefWidth="287.0" text="Parking Signs Status" textFill="#d7c8c8">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
